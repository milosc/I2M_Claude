{
    "$documentation": {
        "purpose": "Registry of stakeholder feedback received during ProductSpecs phase. Tracks feedback items (requirement changes, module adjustments, test additions) through their lifecycle.",
        "stage": "ProductSpecs",
        "phase_position": "Feedback loop - captures and tracks changes to product specification artifacts",
        "upstream": [],
        "downstream": [
            "requirements_registry.json",
            "module_registry.json",
            "productspecs_traceability_register.json"
        ],
        "commands": [
            "/productspecs-feedback",
            "/productspecs-feedback-implement"
        ],
        "skills": [
            "ProductSpecs_FeedbackRegister",
            "ProductSpecs_FeedbackImplementer",
            "ProductSpecs_FeedbackValidator"
        ],
        "folder_structure": {
            "base_path": "traceability/feedback_sessions/productspecs/",
            "session_folder": "{YYYY-MM-DD}_ProductSpecsFeedback_{NNN}/",
            "files": {
                "FEEDBACK_ORIGINAL.md": "Original feedback text",
                "FEEDBACK_SUMMARY.md": "Classified summary",
                "IMPACT_ANALYSIS.md": "Impact assessment",
                "IMPLEMENTATION_PLAN.md": "Change implementation plan",
                "IMPLEMENTATION_LOG.md": "Execution log",
                "VALIDATION_REPORT.md": "Verification results"
            }
        },
        "id_pattern": {
            "format": "PSF-{NNN}",
            "example": "PSF-001"
        },
        "feedback_categories": {
            "CAT-REQ": "Requirement changes",
            "CAT-MOD": "Module changes",
            "CAT-US": "User story updates",
            "CAT-TEST": "Test case additions",
            "CAT-NFR": "NFR modifications"
        },
        "validation_rules": [
            "Every feedback item MUST have a session folder with artifacts",
            "Implemented items MUST update relevant registry files",
            "Session folders MUST follow {YYYY-MM-DD}_ProductSpecsFeedback_{NNN} naming"
        ]
    },
    "$metadata": {
        "schema_version": "1.0.0",
        "created_at": "{{CREATED_AT}}",
        "updated_at": "{{UPDATED_AT}}",
        "generated_by": "Traceability_Initializer"
    },
    "statistics": {
        "total": 0,
        "by_status": {
            "pending": 0,
            "approved": 0,
            "in_progress": 0,
            "implemented": 0,
            "validated": 0,
            "rejected": 0,
            "failed": 0
        },
        "by_category": {},
        "by_type": {}
    },
    "feedback_items": []
}